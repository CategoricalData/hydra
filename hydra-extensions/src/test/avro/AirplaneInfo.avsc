{
  "@label": "engine",
  "@labels": [
    "engine",
    "performance"
  ],
  "namespace": "com.example",
  "name": "AirplaneInfo",
  "type": "record",
  "fields": [
    {
      "name": "id",
      "type": "string"
    },
    {
      "name": "dateAdded",
      "type": "string"
    },
    {
      "@ignore": {},
      "name": "recordNumber",
      "type": [
        "null",
        "string"
      ]
    },
    {
      "@out": "airplane-${name}",
      "name": "airplane",
      "type": {
        "@label": "Airplane",
        "name": "AirplaneModel",
        "type": "record",
        "fields": [
          {
            "name": "name",
            "type": "string"
          },
          {
            "name": "originalManufacturer",
            "type": {
              "name": "Organization",
              "type": "record",
              "fields": [
                {
                  "name": "name",
                  "type": "string"
                },
                {
                  "name": "industry",
                  "type": "string"
                }
              ]
            }
          },
          {
            "name": "currentManufacturer",
            "type": [
              "null",
              "Organization"
            ]
          }
        ]
      }
    },
    {
      "@in": "airplane-${name}",
      "name": "engine",
      "type": {
        "@label": "Engine",
        "name": "EngineModel",
        "type": "record",
        "fields": [
          {
            "name": "name",
            "type": "string"
          },
          {
            "name": "type",
            "type": {
              "name": "EngineType",
              "type": "enum",
              "symbols": [
                "RECIPROCATING",
                "JET",
                "ELECTRIC"
              ]
            }
          }
        ]
      }
    },
    {
      "@performance:in": "performance-${}",
      "name": "performanceProfile",
      "type": "string"
    }
  ]
}